{
  "name": "vuejs-vuex",
  "version": "1.0.0",
  "description": "All I need to start my next vuejs/vuex app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "predist": "rm -fr dist && mkdir dist && mkdir dist/js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "dist": "webpack --config webpack.prod.config.js --progress --colors && npm run dist:min && npm run dist:html",
    "dist:min": "minify dist/js/ --out-dir dist/js/",
    "dist:html": "htmlprocessor ./index.html -o dist/index.html",
    "test": "jest --config jest.config.json"
  },
  "keywords": [
    "vuejs",
    "vuex"
  ],
  "author": "Florent DUVEAU",
  "license": "ISC",
  "dependencies": {
    "leaflet": "^1.3.1",
    "lodash": "4.17.5",
    "vue": "2.5.16",
    "vue-form": "4.8.1",
    "vue-google-maps": "^0.1.21",
    "vue-router": "3.0.1",
    "vue2-leaflet": "^1.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.13",
    "babel-core": "6.26.0",
    "babel-env": "2.4.1",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-minify": "0.3.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "htmlprocessor": "0.2.6",
    "jest": "22.4.3",
    "prettier": "1.11.1",
    "regenerator-runtime": "0.11.1",
    "restify": "7.1.0",
    "style-loader": "0.20.3",
    "vue-jest": "2.3.0",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-livereload-plugin": "2.1.1"
  }
}
